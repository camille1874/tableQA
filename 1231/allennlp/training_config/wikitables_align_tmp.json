        ["fixed_action_embedder.weight|fixed_question_embedder.token_embedder_tokens.weight",
   	{
	"type": "pretrained",
	"weights_file_path":"/home/xuzh/1231/allennlp/1227/best.th",
	"parameter_name_overrides": {
		"fixed_action_embedder.weight": "_action_embedder.weight",
		"fixed_question_embedder.token_embedder_tokens.weight": "_question_embedder.token_embedder_tokens.weight"
	}
	}
    ]
    ]
  },
  "iterator": {
    "type": "basic",
    "batch_size" : 1
  },
  "trainer": {
    "num_epochs": 40,
    "patience": 10,
    "cuda_device": 0,
    "grad_norm": 5.0,
    "validation_metric": "+denotation_acc",
    "optimizer": {
      "type": "sgd",
      "lr": 0.1
    },
    "learning_rate_scheduler": {
      "type": "exponential",
      "gamma": 0.99
    }
  }
