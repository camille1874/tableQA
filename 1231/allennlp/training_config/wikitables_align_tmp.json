  "validation_data_path": "/home/xuzh/1231/data/WikiTableQuestions/data/random-split-1-dev.examples",
  "model": {
    "type": "wikitables_variable_free_mml",
    "question_embedder": {
      "tokens": {
        "type": "embedding",
        "embedding_dim": 200,
        "trainable": true
      }
    },
    "fixed_question_embedder": {
      "tokens": {
        "type": "embedding",
        "embedding_dim": 200,
        "trainable": false
      }
    },
    "action_embedding_dim": 200,
    "encoder": {
      "type": "lstm",
      "input_size": 400,
