{
  "random_seed": 4536,
  "numpy_seed": 9834,
  "pytorch_seed": 953,
  "dataset_reader": {
    "type": "wikitables_variable_free",
    "tables_directory": "/home/geshi/nsm_allen/WikiTableQuestions/",
    "offline_logical_forms_directory": "/home/geshi/nsm_allen/lgfs_new1/",
    "keep_if_no_logical_forms": false,
    "max_offline_logical_forms": 50,
    "lazy": false
  },
  "validation_dataset_reader": {
    "type": "wikitables_variable_free",
    "tables_directory": "/home/geshi/nsm_allen/WikiTableQuestions/",
    "offline_logical_forms_directory": "/home/geshi/nsm_allen/lgfs_new1/",
    "keep_if_no_logical_forms": true,
    "lazy": false
  },
  "vocabulary": {
    "min_count": {"tokens": 3},
    "tokens_to_add": {"tokens": ["-1"]}
  },
  "train_data_path": "/home/geshi/nsm_allen/WikiTableQuestions/data/random-split-1-train.examples",
  "validation_data_path": "/home/geshi/nsm_allen/WikiTableQuestions/data/random-split-1-dev.examples",
  "model": {
    "type": "wikitables_variable_free_mml",
    "question_embedder": {
      "tokens": {
        "type": "embedding",
        "embedding_dim": 200,
        "trainable": true
      }
    },
    "action_embedding_dim": 100,
    "encoder": {
      "type": "lstm",
      "input_size": 400,
      "hidden_size": 100,
      "bidirectional": true,
      "num_layers": 1
    },
    "entity_encoder": {
      "type": "boe",
      "embedding_dim": 200,
      "averaged": true
    },
    "decoder_beam_search": {
      "beam_size": 10
    },
    "max_decoding_steps": 40,
    "attention": {
      "type": "bilinear",
      "vector_dim": 200,
      "matrix_dim": 200
    },
    "initializer":
    [
        ["_encoder._module.weight.*",
            {
                "type":"orthogonal"
            }
        ],
        [".*bias.*",
            {
                "type":"constant",
                "val": 0
            }
        ]
    ]
    //"dropout": 0.5
  },
  "iterator": {
    "type": "basic",
    "batch_size" : 1
  },
  "trainer": {
    "num_epochs": 40,
    "patience": 10,
    "cuda_device": 0,
    "grad_norm": 5.0,
    "validation_metric": "+denotation_acc",
    "optimizer": {
      "type": "sgd",
      "lr": 0.1
    },
    "learning_rate_scheduler": {
      "type": "exponential",
      "gamma": 0.99
    }
  }
}
